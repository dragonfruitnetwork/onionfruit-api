{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Server": {
    "RedisUpdateChannelName": "onionfruit-web:asset-publish-notification",
    "RemoteAssets": {
      "DownloadUrl": "https://example.com/b/onionfruit-web-assets/download?fileName={0}",
      "ListingUrl": "https://example.com/b/onionfruit-web-assets/list?fields=timeCreated,name",

      "ListingPath": "$.objects[*]",
      "EntryNameKey": "name",
      "EntryDateKey": "timeCreated"
    }
  }
}